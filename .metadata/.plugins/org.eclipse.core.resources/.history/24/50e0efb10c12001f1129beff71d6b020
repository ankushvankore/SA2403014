package com.ArrayDemos;

public class D01InitilizationArray {

	public static void main(String[] args) {
		int marks[] = new int[5];
		
		/*
		 * Index
		 * a. Zero Based (Index starts with 0)
		 * b. +ve number
		 * c. Integer number
		 */
		
		marks[0] = 10;
		marks[1] = 20;
		marks[2] = 30;
		marks[3] = 40;
		marks[4] = 50;
		
		
	}

}
